<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.takes.apps.aiueophone"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="8" />
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.CALL_PHONE"/>

    <application
        android:icon="@drawable/fiftyon_ic"
        android:label="@string/title">
        <activity
            android:name=".AIUEOPhoneGyoActivity" android:theme="@style/app_theme" android:screenOrientation="nosensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="AIUEOPhoneDanActivity" android:theme="@style/app_theme" android:screenOrientation="nosensor"></activity>
        <activity android:name="AdressListActivity" android:theme="@style/app_theme" android:screenOrientation="nosensor"></activity>
        <activity android:name="AdressDetailActivity" android:theme="@style/app_theme" android:screenOrientation="nosensor"></activity>
        <activity android:name="MainPreferenceActivity" android:screenOrientation="nosensor"></activity>
    </application>
            
</manifest>